{
    "admin": {
        "export": {
            "default": {
                "properties": [
                    "queueId",
                    "queuedDate",
                    "processed",
                    "processedDate",
                    "from",
                    "to",
                    "subject"
                ],
                "orders": [{
                    "property": "queuedDate",
                    "mode": "desc"
                }],
                "exporterOptions": {
                    "convertBrToNewlines": true,
                    "stripTags": true,
                    "filename": "email-queue-items.csv"
                }
            }
        },

        "defaultExport": "default",

        "lists": {
            "default": {
                "title": {
                    "fr": "Queues de courriel",
                    "en": "Email queue items"
                },
                "properties": [
                    "queueId",
                    "processed",
                    "from",
                    "to"
                ],
                "orders": [{
                    "property": "queued_date",
                    "mode": "desc"
                }]
            }
        },

        "defaultList": "default",

        "forms": {
            "default": {
                "groups": {
                    "charcoal/email/email-queue-item/queue": {
                        "priority": 10
                    },
                    "charcoal/email/email-queue-item/send": {
                        "priority": 30
                    },
                    "charcoal/email/email-queue-item/message": {
                        "priority": 40
                    }
                }
            }
        },

        "defaultForm": "default",

        "formGroups": {
            "charcoal/email/email-queue-item/queue": {
                "title": {
                    "fr": "Queue de courriel",
                    "en": "Email queue"
                },
                "layout": {
                    "structure": [{
                        "columns": [1, 2]
                    }]
                },
                "properties": [
                    "queueId",
                    "queuedDate"
                ]
            },
            "charcoal/email/email-queue-item/send": {
                "title": {
                    "fr": "Envoi",
                    "en": "Sent status"
                },
                "layout": {
                    "structure": [
                        {
                            "columns": [1, 2]
                        },
                        {
                            "columns": [1]
                        },
                        {
                            "columns": [1]
                        },
                        {
                            "columns": [1]
                        }
                    ]
                },
                "properties": [
                    "processed",
                    "processedDate",
                    "processingDate"
                ]
            },
            "charcoal/email/email-queue-item/message": {
                "title": {
                    "en": "Message",
                    "fr": "Message"
                },
                "layout": {
                    "structure": [
                        {
                            "columns": [1, 1]
                        },
                        {
                            "columns": [1]
                        },
                        {
                            "columns": [1]
                        },
                        {
                            "columns": [1]
                        }
                    ]
                },
                "properties": [
                    "from",
                    "to",
                    "subject",
                    "msgHtml",
                    "msgTxt"
                ]
            }
        },

        "dashboards": {
            "admin.edit": {
                "layout": {
                    "structure": [{
                        "columns": [1]
                    }]
                },
                "widgets": {
                    "form": {
                        "type": "charcoal/admin/widget/object-form",
                        "formIdent": "default",
                        "objType": "charcoal/email/email-queue-item"
                    }
                }
            },
            "admin.table": {
                "layout": {
                    "structure": [{
                        "columns": [1]
                    }]
                },
                "widgets": {
                    "form": {
                        "type": "charcoal/admin/widget/table",
                        "collectionIdent": "default",
                        "objType": "charcoal/email/email-queue-item"
                    }
                }
            }
        },
        "defaultCreateDashboard": "admin.edit",
        "defaultEditDashboard": "admin.edit",
        "defaultCollectionDashboard": "admin.table"
    }
}
