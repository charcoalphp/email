{
    "admin": {
        "labels": {
            "name": "Email Log",
            "singularName": "Email Log",
            "editItem": "Edit Email Log",
            "createItem": "Create Email Log",
            "newItem": "New Email Log",
            "allItems": "All Email Logs",
            "menuItems": "Email Logs"
        },
        "export": {
            "default": {
                "properties": [
                    "sendTs",
                    "from",
                    "to",
                    "campaign",
                    "subject",
                    "sendError",
                    "messageId",
                    "ip"
                ],
                "orders": [
                    {
                        "property": "sendTs",
                        "mode": "desc"
                    }
                ],
                "exporterOptions": {
                    "convertBrToNewlines": true,
                    "stripTags": true,
                    "filename": "email-logs.csv"
                }
            }
        },

        "defaultExport": "default",

        "lists": {
            "default": {
                "title": {
                    "fr": "Logs d'envoi de courriel",
                    "en": "Sent Email Logs"
                },
                "properties": [
                    "sendTs",
                    "from",
                    "to",
                    "subject"
                ],
                "orders": [
                    {
                        "property": "sendTs",
                        "mode": "desc"
                    }
                ],
                "pagination": {
                    "numPerPage": 100
                },
                "listActions": {
                    "export": {
                        "url": "object/export?obj_type={{obj_type}}",
                        "label": {
                            "fr": "Exporter",
                            "en": "Export"
                        }
                    }
                }
            }
        },

        "defaultList": "default",

        "forms": {
            "default": {
                "groups": {
                    "charcoal/email/email-log/infos": {
                        "title": {
                            "fr": "Log d'envoi de courriel",
                            "en": "Sent Email Log"
                        },
                        "properties": [
                            "sendTs",
                            "campaign",
                            "from",
                            "to",
                            "subject",
                            "ip",
                            "sessionId"
                        ],
                        "layout": {
                            "structure": [
                                { "columns": [ 1, 1 ] },
                                { "columns": [ 1, 1 ] },
                                { "columns": [ 1 ] },
                                { "columns": [ 1, 1 ] }
                            ]
                        }
                    },
                    "status": {
                        "title": "Status",
                        "properties": [
                            "messageId",
                            "sendStatus",
                            "sendError"
                        ]
                    }
                }
            }
        },

        "defaultForm": "default",

        "dashboards": {
            "admin.edit": {
                "widgets": {
                    "form": {
                        "type": "charcoal/admin/widget/object-form",
                        "formIdent": "default",
                        "objType": "charcoal/email/email-log"
                    }
                },
                "layout": {
                    "structure": [
                        { "columns": [ 1 ] }
                    ]
                }
            },
            "admin.table": {
                "widgets": {
                    "form": {
                        "type": "charcoal/admin/widget/table",
                        "collectionIdent": "default",
                        "objType": "charcoal/email/email-log"
                    }
                },
                "layout": {
                    "structure": [
                        { "columns": [ 1 ] }
                    ]
                }
            }
        },

        "defaultCreateDashboard": "admin.edit",
        "defaultEditDashboard": "admin.edit",
        "defaultCollectionDashboard": "admin.table"
    }
}
